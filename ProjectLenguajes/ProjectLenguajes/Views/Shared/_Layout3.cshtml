  <!-- vehicle section -->

     <section class="about section" id="vehicle">
        <div class="container">
            <div class="row">

                  <div class="d-flex flex-column justify-content-center ml-lg-auto mr-lg-5 col-lg-5 col-md-6 col-12">
                  
                                <div class="team-thumb">
                                    <img src="@Url.Content("~/images/team/honda_civic.jpg")" class="img-fluid" alt="Trainer">


                                    <div class="team-info d-flex flex-column">

                                        <h3>Honda Civic</h3>
                                        <span>VTec Manual</span>

                                      
                                    </div>
                                 </div>

                  
                </div>
                <div class="mr-lg-auto mt-3 col-lg-4 col-md-6 col-12">
                        <div class="entry-form">
                             <form action="" id="vehicle-entry-form">
                                     <h2 class="text-dark" data-aos="fade-up" data-aos-delay="200">Ingresar nuevo vehiculo </h2>
                                   <input type="text" name="id" id="id" hidden>

                                   <label for="idType">Seleccione el tipo de vehiculo: </label>
                                   <select class="form-control without-margin" name="types" id="types">
                                   </select>
                                    

                                   <input type="text" name="brand" id="brand" class="form-control" placeholder="Marca" required="">
                                   
                                   <input type="text" name="model" id="model" class="form-control" placeholder="Modelo" required="">
                                  
                                   <input type="text" name="color" id="color" class="form-control" placeholder="Color" required="">
                                  
                                   <input type="number" name="year" id="year" class="form-control" placeholder="Año" required="">
                                
                                   <input type="text" name="register" id="register" class="form-control" placeholder="identificacion" required="">
                                 
                                   <input type="text" name="description" id="description" class="form-control" placeholder="descripcion" required="">
                                  

                                  @*<button class="submit-btn form-control" onclick="addVehicle();return false" id="addVehicle">Add</button>*@
                                   <button class="submit-btn form-control" onsubmit="return AddVehicle();" id="addvehicle">Añadir vehiculo</button>
                                   <button class="submit-btn form-control" onclick="Clear();return false" id="form-cancel">Cancelar</button>
                                   @*<button class="submit-btn form-control" onsubmit="return Update();" id="form-submit-update">Update</button>*@
                                   @*<input type="submit" name="submit" class="button" id="submit_btn" value="Send" />*@
                                   <label id="result"></label>
                              </form>
                         </div>
                     </div>
                </div>

            </div>
        
        
            <div class="container">
            <div class="row">
                        <h2 class="text-black" data-aos="fade-up" data-aos-delay="200">Vehiculos</h2>
                        <div class="table-responsive">                     
                            <table class="text-black table table-bordered table-responsive schedule-table"  id="vehicles-table"> 
                                <thead class="thead-light">
                                    <tr>
                                        <th>
                                            IDvehicle
                                        </th>
                                        <th>
                                            IDtype
                                        </th>
                                         <th>
                                             Brand
                                        </th>
                                        <th>
                                             Model
                                        </th>
                                        <th>
                                             Color
                                        </th>
                                        <th>
                                             Year
                                        </th>
                                         <th>
                                             register
                                        </th>
                                        <th>
                                             Description
                                        </th>
                                         <th>
                                             Action
                                        </th>
                                    </tr>
                                </thead>

                                <tbody id="vehicle-tbody">
                                    
                                </tbody>

                            </table>


                            <!-- Modal Vehicle -->
                            <div class="modal fade" id="modalVehicle" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                              <div class="modal-dialog modal-dialog-centered" role="document">
                                <div class="modal-content">
                                  <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLongTitle">Update Vehicle</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                      <span aria-hidden="true">&times;</span>
                                    </button>
                                  </div>
                                  <form action="" id="vehicle-entry-form">
                                  <div class="modal-body">

                                       <input type="text" name="idVehicleModal" id="idVehicleModal" hidden>

                                       <label for="idType">Select a vehicle´s type: </label>
                                       <select class="form-control without-margin" name="typesModal" id="typesModal">
                                       </select>
                                        <br>

                                       <input type="text" name="brandModal" id="brandModal" class="form-control" placeholder="Brand" required="">
                                       <br>
                                       <input type="text" name="modelModal" id="modelModal" class="form-control" placeholder="Model" required="">
                                       <br>
                                       <input type="text" name="colorModal" id="colorModal" class="form-control" placeholder="Color" required="">
                                       <br>
                                       <input type="number" name="yearModal" id="yearModal" class="form-control" placeholder="Year" required="">
                                       <br>
                                       <input type="text" name="registerModal" id="registerModal" class="form-control" placeholder="Register of your Vehicle" required="">
                                       <br>
                                       <input type="text" name="descriptionModal" id="descriptionModal" class="form-control" placeholder="Description" required="">
                                       <br>
                                  </div>
                                  <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                    <button type="button" class="btn btn-primary" id="closeModal" onclick="return UpdateVehicle();">Save changes</button>
                                    <label id="modalResult"></label>
                                  </div>
                                  </form>
                                </div>
                              </div>
                            </div>
                            <!-- Modal Vehicle -->                    

                        </div>
                     </div>
            
            </div>
        </div>
    </section>  <!--end of vehicle section -->


    





    <!-- CLIENTS section -->
      <section class="class section" id="clients">
        <div class="container">
            <div class="row">

                <div class="d-flex flex-column justify-content-center ml-lg-auto mr-lg-5 col-lg-5 col-md-6 col-12">
                  
                                <div class="team-thumb">
                                     <img src="@Url.Content("~/images/team/parkingUser.jpg")" class="img-fluid" alt="Trainer">

                                    <div class="team-info d-flex flex-column">

                                        <h3>Pedrito Fernandez</h3>
                                        <span>44 años</span>

                                      
                                    </div>
                                </div>

                  
                </div>

                <div class="mr-lg-auto mt-3 col-lg-4 col-md-6 col-12">
                          <div class="entry-form">
                             <form action="" id="vehicle-entry-form">
                                       <h2 class="text-dark" data-aos="fade-up" data-aos-delay="200">Ingresar Cliente </h2>
                                   <input type="text" name="id" id="id" hidden>

                                    <label for="idVehicle">Elija el vehiculo a registrar: </label>
                                   <select class="form-control without-margin" name="register_v" id="register_v"></select>
                              

                                   <input type="text" name="nameClient" id="nameClient" class="form-control" placeholder="Nombre completo" required="">
                                
                                   <input type="text" name="dniClient" id="dniClient" class="form-control" placeholder="DNI" required="">
                                
                                   <input type="text" name="ageClient" id="ageClient" class="form-control" placeholder="Edad" required="">
                                 
                                   <input type="text" name="telephoneClient" id="telephoneClient" class="form-control" placeholder="Telefono" required="">
                                 
                                   <input type="email" name="emailClient" id="emailClient" class="form-control" placeholder="Email" required="">
                                
                                   <input type="password" name="passwordClient" id="passwordClient" class="form-control" placeholder="Contraseña" required="">
                                 

                                   <button class="submit-btn form-control" onsubmit="return AddClient();" id="form-submit">Añadir Cliente</button>
                                   <button class="submit-btn form-control" onclick="Clear();return false" id="form-cancel">Cancelar</button>
                                   @*<button class="submit-btn form-control" onsubmit="return Update();" id="form-submit-update">Update</button>*@

                                   <label id="result"></label>
                              </form>
                         </div>

                </div>
                </div>

            </div>
        
            <div class="container">
            <div class="row">
                        <h2 class="text-black" data-aos="fade-up" data-aos-delay="200">Cliente</h2>
                        <div class="table-responsive">                     
                            <table class="text-black table table-bordered table-responsive schedule-table"  id="clients-table"> 
                                <thead class="thead-light">
                                    <tr>
                                        <th>
                                            IDclient
                                        </th>
                                        <th>
                                            IDvehicle
                                        </th>
                                         <th>
                                             Name
                                        </th>
                                        <th>
                                             DNI
                                        </th>
                                        <th>
                                             Age
                                        </th>
                                        <th>
                                             Telephone
                                        </th>
                                         <th>
                                             Email
                                        </th>
                                         <th>
                                             Password
                                        </th>
                                        <th>
                                             Rol
                                        </th>
                                        <th>
                                             State
                                        </th>
                                         <th>
                                             Action
                                        </th>
                                    </tr>
                                </thead>

                                <tbody id="client-tbody">
                                    
                                </tbody>

                            </table>


                                              

                        </div>
                     </div>
            
                        </div>
                    
       
      
    </section>  

    <!-- end of CLIENTS section -->

    <!-- RERSERVATIONS -->
    <section id="reservations">
        <div class="container">
            <div class="row">


            
            </div>
        </div>
    </section>
